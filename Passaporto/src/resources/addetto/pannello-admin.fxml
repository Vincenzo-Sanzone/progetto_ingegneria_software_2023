<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.addetto.PannelloAdmin">
   <children>
      <Label fx:id="registrazioneErroreAddetto" />
      <Label text="Inserisci il nome">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="registrazioneNomeAddetto" onKeyPressed="#adminInserisciAddettoEnter">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </TextField>
      <Label text="Inserisci la password">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="registrazionePasswordAddetto" onKeyPressed="#adminInserisciAddettoEnter">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="bottoneInserisciAddetto" mnemonicParsing="false" onAction="#adminInserisciAddetto" text="Inserisci addetto">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Button>
      <Button fx:id="bottoneIndietroPannelloAdmin" mnemonicParsing="false" onAction="#tornaIndietroAdmin" text="Torna indietro">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
